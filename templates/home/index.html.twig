{% extends 'base.html.twig' %}

{% block title %}DeepTube{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">


</div>
    <div class="container">
        {% for musique in musiques %}
        <iframe width="30%" height="300" scrolling="no" frameborder="no" allow="autoplay" src="{{ musique.url }}"></iframe>
            <a href="{{ path('home_remove' , {'id' : musique.id}) }}"><button>Supprimer</button></a>
        {% endfor %}
    </div>

{% endblock %}
